
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css">
<div class="wrapper">
<h1>COVID19<span style="color: rgb(47, 47, 250);">Tracker</span> </h1>
    <p>Search your Country</p>
    <p class="menu">Menu</p>
    <div class="input">
    <input type="text" id="searchBar" placeholder="Enter name of country">
    <button id="searchBtn">Search</button>
    </div>
    <div id="country">
        
    </div>
      <div class="info">
          <div class="box">
              <div class="head">Total Cases</div>
              <div  id="totalCase"  class="counter" >0</div>
              <div class="line"></div>
          </div>
          <div  class="box">
            <div class="head2">Total Death</div>
            <div  id="totalDeath"   class="counter">0</div>
            <div class="line2"></div>
          </div>
          <div class="box">
            <div class="head3">Active Cases</div>
              <div  id="actveCase"  class="counter" >0</div>
              <div class="line3"></div>
          </div>
      </div>
      <div class="info">
          <div class="box">
              <div class="head-1">Recoverd</div>
              <div  id="recoverd" class="counter">0</div>
              <div class="line-1"></div>
          </div>
          <div  class="box">
            <div class="head-2">Critical</div>
            <div  id="critical" class="counter">0</div>
            <div class="line-2"></div>
          </div>
          <div class="box">
            <div class="head-3">Total Tests</div>
              <div  id="tests" class="counter">0</div>
              <div class="line-3"></div>
          </div>
      </div>
      <div id="table" class="container">
        <table class="table table-striped table-responsive-sm">
          <thead>
            <tr id="row-name">
              <th>Name</th>
              <th class="text-danger">Cases</th>
              <th class="text-primary">Active</th>
              <th class="text-seconadry">Deaths</th>
              <th class="text-success">Recoverd</th>
            </tr>
          </thead>
          <tbody id="coloumn-name">
           
          </tbody>
        </table>
      </div>
    </div>
      